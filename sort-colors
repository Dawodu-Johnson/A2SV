class Solution {
public:
    void sortColors(vector<int>& nums) {
        
        // to sort in place we can use bubble sort since we have a small size
        
        for(int i = 0; i < nums.size(); i++){
            
              for(int j = i; j < nums.size(); j++){
                  
                  if(nums[i] > nums[j]){
                       swap(nums[i],nums[j]);
                  }
              }
        }
        
        
    }
};
